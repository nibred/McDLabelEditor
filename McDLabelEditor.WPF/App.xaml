<Application
    x:Class="McDLabelEditor.WPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:McDLabelEditor.WPF"
    xmlns:vmb="clr-namespace:McDLabelEditor.WPF.ViewModels.Base"
    StartupUri="Views/Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vmb:ViewModelLocator x:Key="Locator" />

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Templates/ViewDataTemplates.xaml" />
            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Application.Resources>
</Application>
