<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="clr-namespace:McDLabelEditor.WPF.Views"
    xmlns:vm="clr-namespace:McDLabelEditor.WPF.ViewModels">
    <DataTemplate DataType="{x:Type vm:StartScreenViewModel}">
        <views:StartScreenView DataContext="{Binding}" />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MainEditorViewModel}">
        <views:MainEditorView DataContext="{Binding}" />
    </DataTemplate>
</ResourceDictionary>